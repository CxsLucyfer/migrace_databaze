class Man {
	private String name;

	private String surename;

	private String surename;

	}

	public Man(String name, String surename){
		this(name);
		this.surename = surename;
	public Man(String name){
		this.name = name;
	}

	public Man(String name, String surename){
		this(name);
		this.surename = surename;
	}
}
